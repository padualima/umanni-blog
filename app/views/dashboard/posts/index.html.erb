<%= link_to "Sair", destroy_user_session_path, method: :delete %>
<br><br>

<% @posts.each do |post| %>
<%= post.id %>
<%= post.user.username %>
<%= post.title %>
<%= post.subtitle %>
<%= post.body %>
<%= post.category.description %>
<%= link_to "Editar", edit_dashboard_post_path(post) %>
<%= link_to "Excluir", dashboard_post_path(post), method: :delete,
data: { confirm: "Tem certeza que deseja excluir?"} %>
<br>
<% end %>


<br>
